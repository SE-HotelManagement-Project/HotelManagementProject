    <LinearLayout
        android:id="@+id/llGuestRrInput"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:orientation="vertical"
        android:visibility="visible">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            android:orientation="vertical"
            android:visibility="visible">

            <ImageView
                android:id="@+id/ivAppIcon"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:scaleType="fitCenter"
                app:srcCompat="@drawable/ic_room_available" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="Request Hotel Reservation"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout style="@style/horz_linear_layout_style">

            <TextView
                style="@style/text_style"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingRight="15dp"
                android:text="Hotel Name:" />

            <Spinner
                android:id="@+id/spnrGuestSrHotelName"
                style="@style/Widget.AppCompat.Spinner.Underlined"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:entries="@array/hotel_name_guest"
                android:padding="12dp" />

        </LinearLayout>

        <LinearLayout style="@style/horz_linear_layout_style">

            <TextView
                style="@style/text_style"
                android:text="Check In Date:" />

            <com.google.android.material.textfield.TextInputLayout
                style="@style/edit_text_style"
                android:hint="yyyy-MM-dd format">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/etGuestRrCheckInFrom"
                style="@style/edit_text_style"
                android:hint="yyyy-MM-dd format"
                android:text="" />
            </com.google.android.material.textfield.TextInputLayout>

        </LinearLayout>
        <LinearLayout style="@style/horz_linear_layout_style">
            <TextView
                style="@style/text_style"
                android:text="Start Time:" />


            <com.google.android.material.textfield.TextInputLayout
                style="@style/edit_text_style"
                android:hint="HH:MM (24 Hour Format)">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/etStartTime"
                style="@style/edit_text_style"
                android:text="" />
            </com.google.android.material.textfield.TextInputLayout>
        </LinearLayout>

        <LinearLayout style="@style/horz_linear_layout_style">

            <TextView
                style="@style/text_style"
                android:text="Check Out Date:" />

            <com.google.android.material.textfield.TextInputLayout
                style="@style/edit_text_style"
                android:hint="yyyy-MM-dd format">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/etGuestRrCheckOutDateTo"
                style="@style/edit_text_style"
                android:hint="yyyy-MM-dd format"
                android:text =""/>
            </com.google.android.material.textfield.TextInputLayout>

        </LinearLayout>

        <LinearLayout style="@style/horz_linear_layout_style">

